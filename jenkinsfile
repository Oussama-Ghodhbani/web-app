pipeline {
  agent any 
  
  stages {
    stage('Initialize') {
      steps {
        script {
          echo "PATH = ${PATH}"
          echo "M2_HOME = ${M2_HOME}"
        }
      }
    }
    stage('Build') {
      steps {
        script {
          sh 'mvn clean package'
        }
      }
    }
  }
}
